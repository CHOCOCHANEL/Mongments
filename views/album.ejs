<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Album</title>
</head>
<body>
    <h1>Album</h1>
    <% albums.forEach(function(album) { %>
        <div>
            <h2><%= album.title %></h2>
            <img src="<%= album.image %>" alt="<%= album.title %>">
            <p><%= album.description %></p>
        </div>
    <% }); %>
</body>
</html>
